   "source": [
    "sine_xs=[ 2*math.pi * t/100.0 for t in range(100+1)]\n",
    "sine_ys=[ math.sin(x) for x in sine_xs ]\n",
    "\n",
    "plt.plot(sine_xs,sine_ys,'g-');"
